      <div xml:id="bHY6yh-d34897"><!-- p2l4-r13 -->
        <head xml:id="bHY6yh-d34897-Hd1e108">Rubrica 13</head>
        <head xml:id="bHY6yh-d34897-Hd1e111" type="question-title">De pace seu concordia</head>
        <p xml:id="bHY6yh-d34897-d1e114">
          <lb ed="#Ka"/>de pace et concordia ru<lb ed="#Ka" break="no"/>brica 
          13
        </p>
        <p xml:id="bHY6yh-d34897-d1e119">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e115">Iob hystoria capitulo 22</ref> <quote xml:id="bHY6yh-d34897-Qd1e123">acquieste 
          <lb ed="#Ka"/>omni potenti et habeto pacem 
          <lb ed="#Ka"/>et per hoc habebis fructus 
          <lb ed="#Ka"/>optimos</quote> 
        </p>
        <p xml:id="bHY6yh-d34897-d1e131">
          <pb ed="#Ka" n="74-r"/>
          <cb ed="#Ka" n="a"/>
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e129">PSalmista 33 psalmus</ref> <quote xml:id="bHY6yh-d34897-Qd1e135">Inquire pacem 
          <lb ed="#Ka"/>et persequere eam</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e142">
          <ref xml:id="bHY6yh-d34897-Rd1e140">36 psalmus</ref> // <quote xml:id="bHY6yh-d34897-Qd1e144">Sunt reli<lb ed="#Ka" break="no"/>quie 
          homini pacifico</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e150">
          <ref xml:id="bHY6yh-d34897-Rd1e150">84 psalmus</ref> <quote xml:id="bHY6yh-d34897-Qd1e152">Iustitia 
          <lb ed="#Ka"/>et pax osculate sunt</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e158">
          <ref xml:id="bHY6yh-d34897-Rd1e162">Salomon in prouerbiis capitulo 12</ref> <quote xml:id="bHY6yh-d34897-Qd1e162">Qui pa<lb ed="#Ka" break="no"/>cit 
          in emit consilia sequitur eos g<lb ed="#Ka" break="no"/>audium</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e168">
          <ref xml:id="bHY6yh-d34897-Rd1e172">16 capitulo</ref> <quote xml:id="bHY6yh-d34897-Qd1e170">Cum placuerint domino uie 
          <lb ed="#Ka"/>hominis inimicos quoque eius conuertet
          <lb ed="#Ka"/>ad pacem</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e178">
          <ref xml:id="bHY6yh-d34897-Rd1e186">Cronica de nugis philsophurum 
          <lb ed="#Ka"/>capitulo 7 scilicet de cleobolo</ref> <quote xml:id="bHY6yh-d34897-Qd1e182">Cleobolus vnus 
          <lb ed="#Ka"/>de septem sapientibus dicebat in<lb ed="#Ka" break="no"/>imicum 
          amicum faciend</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e190">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e201">Aristotiles ethycorum libro 8 capitulo 1</ref> // <quote xml:id="bHY6yh-d34897-Qd1e194">Con<lb ed="#Ka" break="no"/>cordia 
          simile aliquem amicitie vi<lb ed="#Ka" break="no"/>detur 
          esse</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e201">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e214">IEsus filius syrach in ecclesiastico capitulo 5</ref>
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d34897-Qd1e205">persequatur te verbum pacis et iustiae</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e211">
          <lb ed="#Ka"/>// <ref xml:id="bHY6yh-d34897-Rd1e224">25 capitulo</ref> <quote xml:id="bHY6yh-d34897-Qd1e213">ait iesus filius syrach In tribus 
          <lb ed="#Ka"/>placitum est spiritui meo que sunt pro<lb ed="#Ka" break="no"/>bata 
          coram deo et hominibus // Con<lb ed="#Ka" break="no"/>cordia 
          fratrum et amor proximorum 
          <lb ed="#Ka"/>et vir et mulier sibi conseutientes</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e225">
          <lb ed="#Ka"/>// <ref xml:id="bHY6yh-d34897-Rd1e240">28 capitulo</ref> <quote xml:id="bHY6yh-d34897-Qd1e227">memento nouissimorum et desine 
          <lb ed="#Ka"/>inimicari abitudo enim et mors im<lb ed="#Ka" break="no"/>minent 
          in mandatis</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e235">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e254">Palustius in gurtiuo</ref> <quote xml:id="bHY6yh-d34897-Qd1e239">Concordia 
          <lb ed="#Ka"/>perue res crescunt discordia magne 
          <lb ed="#Ka"/>dilabuntur</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e245">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e266">Virgilius eneidos libro 11 circa medium</ref>
          <lb ed="#Ka"/>// <quote xml:id="bHY6yh-d34897-Qd1e249">pacem laudate sedentes</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e255">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e278">Tulius de amicitia</ref> <quote xml:id="bHY6yh-d34897-Qd1e259">Quanto uis ami<lb ed="#Ka" break="no"/>citie 
          concordie que sit ex dissensioni<cb ed="#Ka" n="b"/><lb ed="#Ka"/>bus 
          atque discordiis percipi potest</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e268">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e291">Idem philippicorum libro 2</ref> <quote xml:id="bHY6yh-d34897-Qd1e270">nomen pacis 
          <lb ed="#Ka"/>dulce est et ipsa res salutaris // pax 
          <lb ed="#Ka"/>est res transquilla libertas</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e282">
          <ref xml:id="bHY6yh-d34897-Rd1e307">libro 12 
          <lb ed="#Ka"/>et penultimo</ref> <quote xml:id="bHY6yh-d34897-Qd1e284">dulce est nomen pacis res 
          <lb ed="#Ka"/>uero ipsa cum iocunda tum salutaris</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e292">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e321">Ouidius de fastis libro 1</ref> <quote xml:id="bHY6yh-d34897-Qd1e296">Pax te recem 
          <lb ed="#Ka"/>nutrit pacis alumpnatores</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e302">
          <ref xml:id="bHY6yh-d34897-Rd1e330">libro 4</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d34897-Qd1e304">pace teres leta est et nos orate co<lb ed="#Ka" break="no"/>loni 
          perpetuam pacem pacisicumque 
          <lb ed="#Ka"/>ducem</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e312">
          <ref xml:id="bHY6yh-d34897-Rd1e344">Quintilianus declamatione 8 
          <lb ed="#Ka"/>declama</ref> // <quote xml:id="bHY6yh-d34897-Qd1e316">Semper odiorum honestus 
          <lb ed="#Ka"/>occasus est et dum simultates nihil 
          <lb ed="#Ka"/>aliud habent quam nocendi cupidi<lb ed="#Ka" break="no"/>tatem 
          speciosa in melius animi muta<lb ed="#Ka" break="no"/>cio 
          est cum exempli honore</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e326">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e360">Matheus in ewangelio capitulo 5</ref> // <quote xml:id="bHY6yh-d34897-Qd1e330">beati
          <lb ed="#Ka"/>pacifici quoniam ipsi filii dei uocabuntur</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e337">
          <lb ed="#Ka"/>// <ref xml:id="bHY6yh-d34897-Rd1e371">9 capitulo</ref> <quote xml:id="bHY6yh-d34897-Qd1e339">ait ihesus discipulis suis pa<lb ed="#Ka" break="no"/>cem 
          habete inter uos</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e345">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e383">Iohannes in ewangelio capitulo 14</ref> <quote xml:id="bHY6yh-d34897-Qd1e349">Ait ihesus
          <lb ed="#Ka"/>discipulis suis quoniam morti appropiquabat 
          <lb ed="#Ka"/>pacem relinquo uobis pacem meam 
          <lb ed="#Ka"/>do vobis</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e357">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e397">Petrus apostolus epistola 1 capitulo 2</ref> <quote xml:id="bHY6yh-d34897-Qd1e361">Qui vult 
          <lb ed="#Ka"/>uitam diligere et videre dies bonos 
          <lb ed="#Ka"/>coarceat liguam amalo et labia 
          <lb ed="#Ka"/>sua ne loquantur dolum // declinet a<lb ed="#Ka" break="no"/>malo 
          et faciat bonum inquirat pacem 
          <lb ed="#Ka"/>et sequatur eam</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e373">
          <pb ed="#Ka" n="74-v"/>
          <cb ed="#Ka" n="a"/>
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e415">Paulus apostolus epistola ad romanos 
          <lb ed="#Ka"/>capitulo 14</ref> <quote xml:id="bHY6yh-d34897-Qd1e377">Que pacis sunt sectemur</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e383">
          <ref xml:id="bHY6yh-d34897-Rd1e425">Idem 
          <lb ed="#Ka"/>epistola ad colocenses capitulo 3 et penultimo</ref> / <quote xml:id="bHY6yh-d34897-Qd1e385">pax 
          <lb ed="#Ka"/>cristi exultet in cordibus vestris in 
          <lb ed="#Ka"/>qua et uocati estis in vno corpore</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e395">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e440">Idem epistola ad thymotheum capitulo 2</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d34897-Qd1e397">Sectare iustitiam fidem caritatem 
          <lb ed="#Ka"/>pacem</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e404">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e451">Idem epistola ad ebreos 
          <lb ed="#Ka"/>// capitulo 12</ref> <quote xml:id="bHY6yh-d34897-Qd1e406">pacem sequimini cum omnibus 
          <lb ed="#Ka"/>sine qua nemo uidebit deum</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e414">
          <ref xml:id="bHY6yh-d34897-Rd1e466">Seneca tragediarum in hercule fu<lb ed="#Ka" break="no"/>rente 
          capitulo 2</ref>
        </p>
        <p xml:id="bHY6yh-d34897-d1e422">
          <quote xml:id="bHY6yh-d34897-Qd1e426">pacem reduci uelle 
          <lb ed="#Ka"/>uictor expedit uicto necesse esse</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e432">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e487">Idem epistolarum ad lucillum libro 8 epistola 
          <lb ed="#Ka"/>3</ref> // <quote xml:id="bHY6yh-d34897-Qd1e434">non est dubium quin felicior pax 
          <lb ed="#Ka"/>sit nunquam lacessita quam multo 
          <lb ed="#Ka"/>reparata sanguine</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e444">
          <ref xml:id="bHY6yh-d34897-Rd1e502">Idem de ira libro 2</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d34897-Qd1e446">Quid est gloriosius quam iram in ami<lb ed="#Ka" break="no"/>ciciam
          commutare</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e452">
          ¶ <ref xml:id="bHY6yh-d34897-Rd1e514">Idem in prouer<lb ed="#Ka" break="no"/>biis</ref> 
          // <quote xml:id="bHY6yh-d34897-Qd1e456">pacem cum omnibus habeas 
          <lb ed="#Ka"/>bellum cum vitiis semper dissensio ab 
          <lb ed="#Ka"/>alio incipiat ate reconciliatio</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e465">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e529">Ualerius maximus libro 1 titulo de insti<lb ed="#Ka" break="no"/>tutis
          antiquis</ref> // <quote xml:id="bHY6yh-d34897-Qd1e469">Conuiuium solempne 
          <lb ed="#Ka"/>mayores instituerunt idque ca<lb ed="#Ka" break="no"/>ristiam 
          appellauerunt cui preter 
          <lb ed="#Ka"/>congnatos et affines nemo inter<lb ed="#Ka" break="no"/>ponebatur 
          ut si qua inter neces<lb ed="#Ka" break="no"/>sarios 
          personas querela esset or<lb ed="#Ka" break="no"/>ta 
          apud facta mense et inter 
          <cb ed="#Ka" n="b"/>
          <lb ed="#Ka"/>illaritatem animorum a fautoribus 
          <lb ed="#Ka"/>concordie tollerentur</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e494">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e559">libro 4 titulo 2</ref> <quote xml:id="bHY6yh-d34897-Qd1e496">sicut 
          <lb ed="#Ka"/>placidum mare ex asperso celumque 
          <lb ed="#Ka"/>ex nubilo serenum hiberi asspectu 
          <lb ed="#Ka"/>sentitur sic bellum pace mutatum 
          <lb ed="#Ka"/>plurimum gaude affert Speciosius 
          <lb ed="#Ka"/>alii iniurie beneficio uincuntur quam 
          <lb ed="#Ka"/>mutui odii pertinacia pensantur</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e510">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e579">Prudentius de pisto machia capitulo 10</ref> 
          <lb ed="#Ka"/>// <quote xml:id="bHY6yh-d34897-Qd1e514">pax plenum virtutis opus pax 
          <lb ed="#Ka"/>summa laborum // Pax belli exacti 
          <lb ed="#Ka"/>precium est preciumque pericli nil placidum 
          <lb ed="#Ka"/>sine pace deo</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e524">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e595">Prosper sub ultimo capitulo</ref> <quote xml:id="bHY6yh-d34897-Qd1e528">Insontem
          <lb ed="#Ka"/>vitam pacis amator agit</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e534">
          <ref xml:id="bHY6yh-d34897-Rd1e607">Cassi<lb ed="#Ka" break="no"/>odorus 
          epistolarum libro 5 epistola 43</ref> <quote xml:id="bHY6yh-d34897-Qd1e538">Non 
          <lb ed="#Ka"/>est leue prudentes viros in pacis 
          <lb ed="#Ka"/>constituta peccare</quote> //
        </p>
        <p xml:id="bHY6yh-d34897-d1e546">
          OMITTED
        </p>
        <p xml:id="bHY6yh-d34897-d1e555">
          <ref xml:id="bHY6yh-d34897-Rd1e630">libro 12 et finale epistola 3</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d34897-Qd1e557">viri fortes semper in pace modesti
          <lb ed="#Ka"/>sunt</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e563">
          OMITTED
        </p>
        <p xml:id="bHY6yh-d34897-d1e573">
          OMITTED
        </p>
        <p xml:id="bHY6yh-d34897-d1e579">
          <ref xml:id="bHY6yh-d34897-Rd1e662">Matheus vindocinensis 
          <lb ed="#Ka"/>de hystoria thobie capitulo 40</ref> // <quote xml:id="bHY6yh-d34897-Qd1e583">Esto pa<lb ed="#Ka" break="no"/>cificus 
          fuge iurgia vincula pacis 
          <lb ed="#Ka"/>Celica sunt anime pocula corpus 
          <lb ed="#Ka"/>alunt plausus alit gentes</quote>
        </p>
        <p xml:id="bHY6yh-d34897-d1e593">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d34897-Rd1e681">Prouerbium uulgare</ref> <quote xml:id="bHY6yh-d34897-Qd1e597">Rason mete 
          <lb ed="#Ka"/>pace // Chi non se nol inganare 
          <lb ed="#Ka"/>tosto se acorda beato se proua 
          <lb ed="#Ka"/>chi eu pasce se troua</quote>
        </p>
      </div>
