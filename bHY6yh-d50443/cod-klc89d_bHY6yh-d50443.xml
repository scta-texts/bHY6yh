      <div xml:id="bHY6yh-d50443"><!-- p3l2-r4 -->
        <head xml:id="bHY6yh-d50443-Hd1e108">Rubrica 4</head>
        <head xml:id="bHY6yh-d50443-Hd1e111" type="question-title">De consilio</head>
        <p xml:id="bHY6yh-d50443-d1e114">
          <lb ed="#Ka"/>de Consilio Rubrica 4
        </p>
        <p xml:id="bHY6yh-d50443-d1e121">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e120">SAlomon in prouerbiis</ref> <quote xml:id="bHY6yh-d50443-Qd1e127">vbi 
          <lb ed="#Ka"/>non est gubernator populus 
          <lb ed="#Ka"/>corruet Salus autem vbi mul<lb ed="#Ka" break="no"/>ta 
          consilia</quote> 
        </p>
        <p xml:id="bHY6yh-d50443-d1e135">
          <ref xml:id="bHY6yh-d50443-Rd1e134">12 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e137">Consilia impiorum 
          <lb ed="#Ka"/>fraudulenta via stulti recta in oc<lb ed="#Ka" break="no"/>culis 
          eius qui autem sapiens est au<lb ed="#Ka" break="no"/>diet 
          consilia</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e148">
          <ref xml:id="bHY6yh-d50443-Rd1e150">13 capitulo <sic>capitulo</sic></ref> <quote xml:id="bHY6yh-d50443-Qd1e150">Qui cum consilia 
          <pb ed="#Ka" n="109-r"/>
          <cb ed="#Ka" n="a"/>
          <lb ed="#Ka"/>cuncta agunt reguntur sapientia / astutus 
          <lb ed="#Ka"/>omnia agit cum consilio</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e158">
          <ref xml:id="bHY6yh-d50443-Rd1e162">20 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e160">Cogitatio<lb ed="#Ka" break="no"/>nes 
          consiliis laborantaur</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e166">
          <ref xml:id="bHY6yh-d50443-Rd1e172">24 capitulo</ref> //
        </p>
        <p xml:id="bHY6yh-d50443-d1e174">
          <quote xml:id="bHY6yh-d50443-Qd1e176">causam 
          <lb ed="#Ka"/>tuam tracta cum amico tuo</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e180">
          // <ref xml:id="bHY6yh-d50443-Rd1e192">Thobias 
          <lb ed="#Ka"/>capitulo 4</ref> <quote xml:id="bHY6yh-d50443-Qd1e184">Consilium a sapiente semper perquire</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e190">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e204">Socrates in exortationibus capitulo 2</ref> <quote xml:id="bHY6yh-d50443-Qd1e194">Con<lb ed="#Ka" break="no"/>silians 
          fac exemplum praeterita futurorum 
          <lb ed="#Ka"/>Nam ignoti ex noto coniectura ci<lb ed="#Ka" break="no"/>tissima 
          nascitur // Consiliare quid diutius 
          <lb ed="#Ka"/>effice uero uisa uelociter // Cum protius te 
          <lb ed="#Ka"/>expedit consulere quemquam cerne prius 
          <lb ed="#Ka"/>quodo disposuerit sua // nam qui perpe<lb ed="#Ka" break="no"/>ram 
          patitur in propriis nunquam bene 
          <lb ed="#Ka"/>consiliabitur in alienis // Sic ad potius 
          <lb ed="#Ka"/>consiliandum incitaberis si calamita<lb ed="#Ka" break="no"/>tes 
          prouenientes ex consiliis carenti<lb ed="#Ka" break="no"/>a 
          prospexeris nam et sanitatis ma<lb ed="#Ka" break="no"/>ge 
          sedulissimus cum miseritis lang
          <lb ed="#Ka"/>ivoris adultinus</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e227">
          <ref xml:id="bHY6yh-d50443-Rd1e243">Aristotiles ethico<lb ed="#Ka" break="no"/>rum 
          libro 3 capitulo 4</ref> ¶ <quote xml:id="bHY6yh-d50443-Qd1e231">Consilium omne questio est</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e237">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e253">libro 6 capitulo 3</ref> // <quote xml:id="bHY6yh-d50443-Qd1e239">Non est consilia<lb ed="#Ka" break="no"/>ari 
          de hiis que ex neccessitate sunt</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e245">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e263">5 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e247">Prudentis hoc maxime opus 
          <lb ed="#Ka"/>esse dicimus bene consiliari ¶ Con<lb ed="#Ka" break="no"/>siliatur 
          nullus de impossibilibus ali<lb ed="#Ka" break="no"/>ter 
          se habere neque de quibuscumque non 
          <lb ed="#Ka"/>finis aliquis est ¶ Simpliciter 
          <lb ed="#Ka"/>bonus consiliator est optimi omni<lb ed="#Ka" break="no"/>um 
          operabilium coniectatiuus secundum 
          <lb ed="#Ka"/>ratiocinationem cConsiliari querem 
          <cb ed="#Ka" n="b"/>
          <lb ed="#Ka"/>quoddam est</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e266"> 
          <ref xml:id="bHY6yh-d50443-Rd1e286">6 capitulo</ref> // <quote xml:id="bHY6yh-d50443-Qd1e268">Consilians querit 
          <lb ed="#Ka"/>et ratiocinatur // Operari quidem et
          <lb ed="#Ka"/>rotinatur operari quidem opertet 
          <lb ed="#Ka"/>felociter consiliata consiliari autem 
          <lb ed="#Ka"/>corde</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e276">  
          <ref xml:id="bHY6yh-d50443-Rd1e298">Idem rethoricorum libro 1 capitulo 2</ref> <quote xml:id="bHY6yh-d50443-Qd1e278">Consilia<lb ed="#Ka" break="no"/>mur 
          de hiis que videntur contingere 
          <lb ed="#Ka"/>vtroque modo se habere</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e286">
          <ref xml:id="bHY6yh-d50443-Rd1e311">17 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e288">Ex prius
          <lb ed="#Ka"/>factis futura diiudicantes diuidicamus</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e295">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e322">libro 2 capitulo 7</ref> <quote xml:id="bHY6yh-d50443-Qd1e299">Timor consiliatiuos facit</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e303">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e333">Idem politicorum libro 4 capitulo 11</ref> <quote xml:id="bHY6yh-d50443-Qd1e305">Consiliantur 
          <lb ed="#Ka"/>melius communiter consiliantes omnes</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e313">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e348">Iesus filius syrach in ecclesiastico capitulo 6</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e317">Multi pacifici sint tibi et consiliarius 
          <lb ed="#Ka"/>vnus de mille</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e323">
          <ref xml:id="bHY6yh-d50443-Rd1e360">capitulo 8</ref> <quote xml:id="bHY6yh-d50443-Qd1e327">Coram extraneo 
          <lb ed="#Ka"/>ne facias consilium nescit enim 
          <lb ed="#Ka"/>quid pariet</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e333">
          <ref xml:id="bHY6yh-d50443-Rd1e370">32 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e335">Sine consilio
          <lb ed="#Ka"/>nichil facias et post factum non 
          <lb ed="#Ka"/>penitebis</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e341">
          <ref xml:id="bHY6yh-d50443-Rd1e382">37 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e345">Ante omnem actum 
          <lb ed="#Ka"/>stabile consilium precedat te</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e352">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e395">Therencius in tuniuco capitulo 2</ref> <quote xml:id="bHY6yh-d50443-Qd1e356">dum est 
          <lb ed="#Ka"/>tempus etiam cogita</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e362">
          <ref xml:id="bHY6yh-d50443-Rd1e405">18 capitulo</ref> <quote xml:id="bHY6yh-d50443-Qd1e364">Omnia 
          <lb ed="#Ka"/>plus consilia experiri quam armis 
          <lb ed="#Ka"/>sapientem decet</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e413">
          <ref xml:id="bHY6yh-d50443-Rd1e417">In heauton capitulo 20</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e419">Nonne id flacium est te aliis consi<lb ed="#Ka" break="no"/>lium 
          dare foris sapere tibi non pos<lb ed="#Ka" break="no"/>se 
          te auxiliare</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e376">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e431">Salustius in catilinario</ref> / <quote xml:id="bHY6yh-d50443-Qd1e380">prius quam
          <lb ed="#Ka"/>incipias consulto et ubi consulu<lb ed="#Ka" break="no"/>eris 
          nature opus factum est omnes 
          <pb ed="#Ka" n="109-v"/>
          <cb ed="#Ka" n="a"/>
          <lb ed="#Ka"/>homines qui de rebus consultant dub<lb ed="#Ka" break="no"/>iis 
          ab odio amicitia ira atque miseri
          <lb ed="#Ka"/>cordia vacuos esse decet hand faci<lb ed="#Ka" break="no"/>le 
          animus rerum prouidet vbi ista of<lb ed="#Ka" break="no"/>ficiunt</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e394">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e451">Tulius rethoricorum artis ueteris libro 1 capitulo 12</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e398">Consilium est aliquem faciendi ratione 
          <lb ed="#Ka"/>excogitata ratio</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e406">
          ¶ <ref xml:id="bHY6yh-d50443-Rd1e463">Iud philippicorum 
          <lb ed="#Ka"/>libro 6</ref> // <quote xml:id="bHY6yh-d50443-Qd1e408">Etiam summi gubernatores 
          <lb ed="#Ka"/>in magnis tempestatibus auctori<lb ed="#Ka" break="no"/>bus 
          amoueri solent</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e416">
          <ref xml:id="bHY6yh-d50443-Rd1e476">libro ii</ref> <quote xml:id="bHY6yh-d50443-Qd1e418">optimus 
          <lb ed="#Ka"/>est portus penitenti mutatio con<lb ed="#Ka" break="no"/>silii</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e431">
          // <ref xml:id="bHY6yh-d50443-Rd1e494">Idem in oratione promarce m<lb ed="#Ka" break="no"/>arcello</ref> 
          <quote xml:id="bHY6yh-d50443-Qd1e435">nunquam temeritas cum sap<lb ed="#Ka" break="no"/>ientia 
          commiscetur nec ad consilium 
          <lb ed="#Ka"/>casus admittitur // Iracundia est inimi<lb ed="#Ka" break="no"/>ca 
          consilio</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e445">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e510">Ouidius de arte amandi libro 1</ref> <quote xml:id="bHY6yh-d50443-Qd1e449">Noc<lb ed="#Ka" break="no"/>te 
          latent mende uitioque igno<lb ed="#Ka" break="no"/>scitur 
          omni hora que formosam 
          <lb ed="#Ka"/>quamlibet illa facit / Consule de gem<lb ed="#Ka" break="no"/>mis 
          detincta murice laua Con<lb ed="#Ka" break="no"/>sule 
          de facile corporibus que diem</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e461">
          <lb ed="#Ka"/>// <ref xml:id="bHY6yh-d50443-Rd1e528">Idem sine titulo seu amorum 
          <lb ed="#Ka"/>libro 1 capitulo 3</ref> // <quote xml:id="bHY6yh-d50443-Qd1e465">Consilium nobis resque 
          <lb ed="#Ka"/>locusque dabunt</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e471">
          OMITTED
        </p>
        <p xml:id="bHY6yh-d50443-d1e481">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e552">Matheus in ewangelio capitulo 5</ref> <quote xml:id="bHY6yh-d50443-Qd1e485">Cecus 
          <lb ed="#Ka"/>si ceco ducatum praestet ambo 
          <lb ed="#Ka"/>in foueam cadunt</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e491">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e565">Seneca epistolarum ad lucillum libro 2
          <lb ed="#Ka"/>epistola 5</ref> <quote xml:id="bHY6yh-d50443-Qd1e495">Consilium rerum omnium sapiens 
          <cb ed="#Ka" n="b"/>
          <lb ed="#Ka"/>spectat non exitium</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e504">
          <ref xml:id="bHY6yh-d50443-Rd1e577">libro 17 epistola 3</ref> <quote xml:id="bHY6yh-d50443-Qd1e506">Quid<lb ed="#Ka" break="no"/>quid 
          consilio prospicere potes prospice</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e512">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e589">Lex codatis titulo de repudio libro 3</ref> <quote xml:id="bHY6yh-d50443-Qd1e516">dubium 
          <lb ed="#Ka"/>non est omnia que consilio ratione ge<lb ed="#Ka" break="no"/>runtur 
          iure meritoque effectum et fir<lb ed="#Ka" break="no"/>mitate 
          uiti</quote>
        </p>
        <p xml:id="bHy6yh-d50443-d1e522">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e602">Marcialis cocus libro vndique suscepto</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e526">pannorum ueterum facile contempnitur 
          <lb ed="#Ka"/>vsus / Non sic consilium posthume 
          <lb ed="#Ka"/>superne scenum
          <lb ed="#Ka"/>Consilio inuenum fidis me libro
          <lb ed="#Ka"/>ruinam Expectare potes de re 
          <lb ed="#Ka"/>militari</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e535">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e620">Uegetius de re militari libro 1 capitulo 26
          <lb ed="#Ka"/>et fine</ref> / <quote xml:id="bHY6yh-d50443-Qd1e539">nulla consilia sunt meliora 
          <lb ed="#Ka"/>quam illa que ignorauerit aduersarius 
          <lb ed="#Ka"/>ante quam facias Cum consilium tuum cog<lb ed="#Ka" break="no"/>nouerit 
          aduersarius proditum dis<lb ed="#Ka" break="no"/>positionem 
          mutare te conuenit</quote> //
        </p>
        <p xml:id="bHY6yh-d50443-d1e551">
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e553">Quot fieri debeat tractato cum multis quod 
          <lb ed="#Ka"/>non facturus sis cum paucissimis 
          <lb ed="#Ka"/>vel potius ipse tecum</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e561">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e651">Ualerius maximus libro 7 titulo fine et dicta
          <lb ed="#Ka"/>aut facta</ref>: <quote xml:id="bHY6yh-d50443-Qd1e565">hastrubal a Iulio pro<lb ed="#Ka" break="no"/>consule 
          nauali prelio uictus timens
          <lb ed="#Ka"/>que classis amisse penas dari Offen<lb ed="#Ka" break="no"/>sam 
          astutia auertit Nam ex illa 
          <lb ed="#Ka"/>infelici pugna prius quam cladis 
          <lb ed="#Ka"/>nuncius domum perueniret quendam 
          <lb ed="#Ka"/>ex amicis compositum et ornatum 
          <lb ed="#Ka"/>cartaginem misit qui postquam
          <pb ed="#Ka" n="110-r"/>
          <cb ed="#Ka" n="a"/>
          <lb ed="#Ka"/>ciuitatis eius curiam intrauit con<lb ed="#Ka" break="no"/>sulit 
          nos inquit hasdrubal cum dux 
          <lb ed="#Ka"/>romanorum magnas secum mariti<lb ed="#Ka" break="no"/>mas 
          trahens setum copias aduenerit. 
          <lb ed="#Ka"/>an cum eo confligere debeat ac cla<lb ed="#Ka" break="no"/>mouit 
          vniuersus senatus non esse 
          <lb ed="#Ka"/>dubium quin oporteret cum ille con<lb ed="#Ka" break="no"/>flixit 
          inquid de factum dampnare 
          <lb ed="#Ka"/>quod ipsi fieri debere iudicauerunt</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e601">
          <ref xml:id="bHY6yh-d50443-Rd1e692">libro 
          <lb ed="#Ka"/>8 titulo sue Quemque artis etc</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e603">Quintus sceuola legum clarissimus 
          <lb ed="#Ka"/>et certissimus uates quotienscumque 
          <lb ed="#Ka"/>de iure praetorio consulebatur ad furium 
          <lb ed="#Ka"/>et caselium quia huic scientie dedi<lb ed="#Ka" break="no"/>ti 
          erant consultores reiciebat plato 
          <lb ed="#Ka"/>conductores sacrare modum et form<lb ed="#Ka" break="no"/>am
          eius secum sermone conferre cona<lb ed="#Ka" break="no"/>tus 
          ad euclidem geometram ire ius<lb ed="#Ka" break="no"/>sit 
          scientie eius scedens ymo professioni</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e629">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e724">Auctor de forma honeste vite capitulo 2</ref>
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e633">prudentis proprium est consilia exami<lb ed="#Ka" break="no"/>nare 
          et non cito facili credulitate 
          <lb ed="#Ka"/>ad falsa prolabi</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e641">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e738">Ambrosius de officiis libro 1 capitulo 31</ref> // <quote xml:id="bHY6yh-d50443-Qd1e645">vnus<lb ed="#Ka" break="no"/>quisque 
          beniuolentis se magis quam 
          <lb ed="#Ka"/>sapientis credit consilio</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e653">
          <ref xml:id="bHY6yh-d50443-Rd1e750">libro 2 capitulo 8</ref> // 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e655">promptius nos iusto uiro quam 
          <lb ed="#Ka"/>prudenti committimus</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e661">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e762">Macrobius saturnaliorum libro 1 capitulo 15</ref> // 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e665">Conceptu mentis consilia nascuntur</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e671">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e775">Cato capitulo 1</ref> <quote xml:id="bHY6yh-d50443-Qd1e675">ne accesseris ad consilium 
          <cb ed="#Ka" n="b"/>
          <lb ed="#Ka"/>antequam uoceris Tu te consule</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e681">
          <ref xml:id="bHY6yh-d50443-Rd1e785">capitulo 4</ref> 
          <lb ed="#Ka"/><quote xml:id="bHY6yh-d50443-Qd1e683">vtile consilium dominus ne despice 
          <lb ed="#Ka"/>serui si prodest contempseris unquam</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e692">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e799">Cassiodorus epistolarum libro 6 epistola 19</ref> <quote xml:id="bHY6yh-d50443-Qd1e696">omnis 
          <lb ed="#Ka"/>prudens consilium querit</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e702">
          <ref xml:id="bHY6yh-d50443-Rd1e809">libro 10 epistola 4</ref> // <quote xml:id="bHY6yh-d50443-Qd1e704">Con<lb ed="#Ka" break="no"/>silium 
          mutare tractat prouidus // 
          <lb ed="#Ka"/>Sapientiam ille querit in alto penes 
          <lb ed="#Ka"/>quem est scientie magnitudo</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e714">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e825">Gregorius in pastorali capitulo 75</ref> <quote xml:id="bHY6yh-d50443-Qd1e718">Ta<lb ed="#Ka" break="no"/>dius 
          peccatum soluitur quod per consilium 
          <lb ed="#Ka"/>solidatur</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e724">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e837">Uersificator fabularum esopi fabula
          <lb ed="#Ka"/>14</ref> <quote xml:id="bHY6yh-d50443-Qd1e728">prudentia summa est credere pru<lb ed="#Ka" break="no"/>denti</quote> 
        </p>
        <p xml:id="bHY6yh-d50443-d1e734">
          / <ref xml:id="bHY6yh-d50443-Rd1e850">fabula 20</ref> <quote xml:id="bHY6yh-d50443-Qd1e738">vtile consilium 
          <lb ed="#Ka"/>qui spernit in vtile summit</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e744">
          // <ref xml:id="bHY6yh-d50443-Rd1e862">Auctor 
          <lb ed="#Ka"/>liber qui dicitur pamphilus capitulo 64 
          <lb ed="#Ka"/>et fine</ref> <quote xml:id="bHY6yh-d50443-Qd1e748">Conuenit ad nostros modus et 
          <lb ed="#Ka"/>prudentia casus Quotque sequi 
          <lb ed="#Ka"/>deceat querere consilium</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e757">
          <lb ed="#Ka"/><ref xml:id="bHY6yh-d50443-Rd1e876">Gaufridus in poetria capitulo 23</ref> <quote xml:id="bHY6yh-d50443-Qd1e761">Si uis 
          <lb ed="#Ka"/>bene duci te certe committe duci</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e767">
          <ref xml:id="bHY6yh-d50443-Rd1e888">Idem 
          <lb ed="#Ka"/>in hyronia capitulo 13</ref>
        </p>
        <p xml:id="bHY6yh-d50443-d1e777">
          <quote xml:id="bHY6yh-d50443-Qd1e781">Si consulius amico 
          <lb ed="#Ka"/>non queras placere ei sed rationi // di<lb ed="#Ka" break="no"/>cas 
          in consulendo amice sic michi 
          <lb ed="#Ka"/>videtur non precise sic agendum</quote>
        </p>
        <p xml:id="bHY6yh-d50443-d1e798">
          // <ref xml:id="bHY6yh-d50443-Rd1e923">Iacobus beneuentanus capitulo 41</ref> <quote xml:id="bHY6yh-d50443-Qd1e802">fidum consilium 
          <lb ed="#Ka"/>fidus amicus habet</quote>
        </p>
      </div>
    </body>
  </text>
</TEI>