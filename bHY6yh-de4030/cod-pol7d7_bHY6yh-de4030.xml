    <body>
      <div xml:id="bHY6yh-de4030"><!-- p1l1-r15 -->
        <head xml:id="bHY6yh-de4030-Hd1e108">Rubrica 15</head>
        <head xml:id="bHY6yh-de4030-Hd1e111" type="question-title">De oratione religiosa</head>
        <p xml:id="bHY6yh-de4030-d1e114">
          <lb ed="#K" n="74"/>De oratione religiosa / Rubrica 15
        </p>
        <p xml:id="bHY6yh-de4030-d1e119">
          <lb ed="#K" n="75"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e116">Psalmista 33 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e123">Oculi domini super 
            <lb ed="#K" n="76"/>iustos et aures eius in praeces 
            <lb ed="#K" n="77"/>eorum</quote>
        </p>
        <p xml:id="bHY6yh-de4030-xxx19p">
          ¶ <ref xml:id="bHY6yh-de4030-Rxxx12">Idem</ref> <quote xml:id="bHY6yh-de4030-Qxxx87">clamauerunt ius<lb ed="#K" break="no" n="78"/>ti 
            et dominus exaudiuit eos et 
            <lb ed="#K" n="79"/>ex omnibus tribulationibus eorum 
            <lb ed="#K" n="80"/>liberauit eos</quote> 
        </p>
        <p xml:id="bHY6yh-de4030-d1e135">
          /<ref xml:id="bHY6yh-de4030-Rd1e136">Idem 36 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e139">Subditus 
            <lb ed="#K" n="81"/>esto domino et ora eum</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e144">
          / <ref xml:id="bHY6yh-de4030-Rd1e145">Idem 49 psalmus</ref> 
          <lb ed="#K" n="82"/> <quote xml:id="bHY6yh-de4030-Qd1e146">Immola deo sacrificium laudis</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e152">
          <lb ed="#K" n="83"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e156">Idem 50 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e154">Miserere mei deus 
            <pb ed="#K" n="13-r"/>
            <cb ed="#K" n="a"/>
            <lb ed="#K" n="1"/>secundum magnam misericordiam tuam et 
            <lb ed="#K" n="2"/>dicas usque ad <unclear>nisi</unclear> redde mihi letitiam 
            <lb ed="#K" n="3"/>etc inclusive</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e179">
          ¶ <ref>Idem psalmus 70</ref> <quote xml:id="bHY6yh-de4030-Qd1e181">do<lb ed="#K" break="no" n="4"/>mine 
            labia mea aperies et 
            <lb ed="#K" n="5"/>os meum annunciabit laudem tuam</quote> 
        </p>
        <p xml:id="bHY6yh-de4030-d1e187">
          <lb ed="#K" n="6"/> <ref xml:id="bHY6yh-de4030-Rd1e196">69 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e189">Deus in adiutorium 
            <lb ed="#K" n="7"/>meum intende domine ad adiu<lb ed="#K" break="no" n="8"/>uandum 
            me festina</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e197">
          ¶ <ref xml:id="bHY6yh-de4030-Rd1e208">17 psalmus</ref> 
          <lb ed="#K" n="9"/> <quote xml:id="bHY6yh-de4030-Qd1e199">Deus ne elongeris a me
            <lb ed="#K" n="10"/>deus meus in auxilium meum 
            <lb ed="#K" n="11"/>respice</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e205">
          <ref xml:id="bHY6yh-de4030-Rd1e218">Idem psalmus 84</ref> <quote xml:id="bHY6yh-de4030-Qd1e207">Ostende 
            <lb ed="#K" n="12"/>nobis domine misericordiam tuam et sa<lb ed="#K" break="no" n="13"/>lutare 
            tuum da nobis</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e216">
          ¶ <ref xml:id="bHY6yh-de4030-Rd1e231">Idem 85 psalmus</ref> 
          <lb ed="#K" n="14"/> <quote xml:id="bHY6yh-de4030-Qd1e218">Tu deus suauis et mitis et multe 
            <lb ed="#K" n="15"/>mine es omnibus inuocantibus 
            <lb ed="#K" n="16"/>te</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e224">
          /<ref xml:id="bHY6yh-de4030-Rd1e241">Idem 101 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e226">Dominus respexit 
            <lb ed="#K" n="17"/>in oronem humilium et non spre<lb ed="#K" break="no" n="18"/>uit 
            praecem eorum /neque despexit  
            <lb ed="#K" n="19"/>deprecationem pauperis</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e234">
          <ref xml:id="bHY6yh-de4030-Rd1e254">Idem 102</ref> 
          <lb ed="#K" n="20"/> <quote xml:id="bHY6yh-de4030-Qd1e236">Confitemini domino et inuocate 
            <lb ed="#K" n="21"/>nomen eius</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e242">
          /<ref xml:id="bHY6yh-de4030-Rd1e264">Idem 146 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e244">Qui se<lb ed="#K" break="no" n="22"/>minat 
            in lachrymis in exultatione 
            <lb ed="#K" n="23"/>metet</quote> 
        </p>
        <p xml:id="bHY6yh-de4030-d1e250">
          <ref xml:id="bHY6yh-de4030-Rd1e274">Idem 158</ref> <quote xml:id="bHY6yh-de4030-Qd1e252">Domine misericordia tua
            <lb ed="#K" n="24"/>in saeculum et opera manuum tuarum 
            <lb ed="#K" n="25"/>ne despicias</quote> 
        </p>
        <p xml:id="bHY6yh-de4030-d1e258">
          ¶ <ref xml:id="bHY6yh-de4030-Rd1e285">Idem 161 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e260">Po<lb ed="#K" break="no" n="26"/>ne 
            domine custodiam ori meo 
            <lb ed="#K" n="27"/>et hostium circunstantie labiis 
            <lb ed="#K" n="28"/>meis</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e269">
          /<ref xml:id="bHY6yh-de4030-Rd1e299">Idem 163 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e271">Non intres 
            <lb ed="#K" n="29"/>in iudicium cum seruo tuo quia non 
            <lb ed="#K" n="30"/>iustificabitur in conspectum tuo omnis 
            <lb ed="#K" n="31"/>viuens</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e279">
          ¶ <ref xml:id="bHY6yh-de4030-Rd1e314">Idem 165 psalmus</ref> <quote xml:id="bHY6yh-de4030-Qd1e283">Prope est 
            <lb ed="#K" n="32"/>dominus omnibus inuocantibus 
            <lb ed="#K" n="33"/>eum in veritate voluntatem timent 
            <lb ed="#K" n="34"/>ium se faciet et depre<lb ed="#K" break="no" n="35"/>cationem 
            eorum exandiet et 
            <lb ed="#K" n="36"/>saluos faciet eos</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e293">
          <lb ed="#K" n="37"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e330">Salomon in prouerb capitulo 15</ref> <quote xml:id="bHY6yh-de4030-Qd1e297">Longe 
            <lb ed="#K" n="38"/>est dominus ab impiis et orationes 
            <lb ed="#K" n="39"/>iustorum exaudiet omni</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e307">
          <lb ed="#K" n="40"/> <ref xml:id="bHY6yh-de4030-Rd1e346">Tobie hysto capitulo 4</ref> <quote xml:id="bHY6yh-de4030-Qd1e311">Omnitempore 
            <cb ed="#K" n="b"/>
            <lb ed="#K" n="41"/>benedic deum et pete ab eo 
            <lb ed="#K" n="42"/>ut vias tuas dirigat et omnia 
            <lb ed="#K" n="43"/>confiliatua in ipso permaneant</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e319">
          <lb ed="#K" n="44"/> /<ref xml:id="bHY6yh-de4030-Rd1e358">Idem 12 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e321">Bona est oratio 
            <lb ed="#K" n="45"/>cum ieiunio et elemosyna magis 
            <lb ed="#K" n="46"/>quam thesauros auri condere</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e329">
          <lb ed="#K" n="47"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e372">Iesus filius sirach in ecclesia capitulo 3</ref> 
          <lb ed="#K" n="48"/> <quote xml:id="bHY6yh-de4030-Qd1e333">Qui diligit deum exorabit 
            <lb ed="#K" n="49"/>pro peccatis et continens est ab 
            <lb ed="#K" n="50"/>illis et in oratione dierum exaudi<lb ed="#K" break="no" n="51"/>etur</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e342">
          /<ref xml:id="bHY6yh-de4030-Rd1e385">Idem 18 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e344">Ante orationem praepara 
            <lb ed="#K" n="52"/>animam tuam et noli esse 
            <lb ed="#K" n="53"/>quasi homo qui tentat deum</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e352">
          <lb ed="#K" n="54"/> <ref xml:id="bHY6yh-de4030-Rd1e399">Mattheus in euange 6 capitulo </ref> <quote xml:id="bHY6yh-de4030-Qd1e356">cum omnia  non eritis 
            <lb ed="#K" n="55"/>sicut hypocrite qui amant in 
            <lb ed="#K" n="56"/>synagogis et angulis pla<lb ed="#K" break="no" n="57"/>tearum 
            stantes orare ut videan<lb ed="#K" break="no" n="58"/>tur 
            ab hominibus Tu auctor 
            <lb ed="#K" n="59"/>cum oratibus intra cubiculum <!--repeated word?-->
            <lb ed="#K" n="60"/>cubiculum tuum et clauso ostio 
            <lb ed="#K" n="61"/>tuo ora patrem tuum in ab<lb ed="#K" break="no" n="62"/>scondito 
            et pater tuus qui videt 
            <lb ed="#K" n="63"/>in abscondito reddet tibi oran<lb ed="#K" break="no" n="64"/>tes 
            nolite multum loqui ¶ <!--not divided in the Venice text-->
            <lb ed="#K" n="65"/> Idem Sic orabitis pater nitur qui 
            <lb ed="#K" n="66"/>es icelis sanctificetur nomen 
            <lb ed="#K" n="67"/>tuum adueniat regnum tuum 
            <lb ed="#K" n="68"/>fiat voluntas tua sicut in 
            <lb ed="#K" n="69"/>celo et in terra panem nostrum 
            <lb ed="#K" n="70"/>supersubstantialem da nobis hodie 
            <lb ed="#K" n="71"/>et dimitte nobis debita nostra 
            <lb ed="#K" n="72"/>sicut et nos dimit timus debi<lb ed="#K" break="no" n="73"/>toribus 
            nostris et ne nos in<lb ed="#K" break="no" n="74"/>ducas 
            intentationem sed li<lb ed="#K" break="no" n="75"/>bera 
            nos a malo</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e386">
          <lb ed="#K" n="76"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e433">Idem capitulo 7</ref> <quote xml:id="bHY6yh-de4030-Qd1e388">petite et dabitur 
            <lb ed="#K" n="77"/>vobis quaerite et inuenietis 
            <lb ed="#K" n="78"/>pulsate et aperietur vobis</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e396">
          ¶ <ref xml:id="bHY6yh-de4030-Rd1e445">Idem 
            <lb ed="#K" n="79"/>26 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e398">uigilate et orate ut 
              <lb ed="#K" n="80"/>non interetis in tentationem 
              <pb ed="#K" n="13-v"/>
              <cb ed="#K" n="a"/>
              <lb ed="#K" n="1"/>spiritus quidem promptus est caro 
              <lb ed="#K" n="2"/>autem infirma</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e406">
          <lb ed="#K" n="3"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e459">Matheus in euan xi</ref> <quote xml:id="bHY6yh-de4030-Qd1e410">capitulo omnia  quaecumque 
            <lb ed="#K" n="4"/>orantes petitis credite et  
            <lb ed="#K" n="5"/>quia accipietis et veniet vobis / Idem
            <lb ed="#K" n="6"/>cum stabitis ad orandum dimittite 
            <lb ed="#K" n="7"/>si quid habetis aduersus aliquem ut 
            <lb ed="#K" n="8"/>pater vester qui in celis est dimit<lb ed="#K" break="no" n="9"/>tat 
            vobis peccata vestra</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e422">
          <lb ed="#K" n="10"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e477">Lucas in euan 11 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e426">Cum oratis di<lb ed="#K" break="no" n="11"/>cite 
            pater sanctificetur nomen tuum ad<lb ed="#K" break="no" n="12"/>ueniat 
            regnum tuum panem nostrum corda no<lb ed="#K" break="no" n="13"/>bis 
            homine et dimitte nobis pec<lb ed="#K" break="no" n="14"/>unia
            fidem et ipsi dimittimus omnibus
            <lb ed="#K" n="15"/>debita nostra et ne nos in<lb ed="#K" break="no" n="16"/>ducas 
            in tentationem</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e441">
          ¶ <ref xml:id="bHY6yh-de4030-Rd1e496">Idem 22 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e443">Orate 
            <lb ed="#K" n="17"/>ne intretis intentationem</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e449">
          <lb ed="#K" n="18"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e508">Ioannes in euangel capitulo 9</ref> <quote xml:id="bHY6yh-de4030-Qd1e453">Pecca<lb ed="#K" break="no" n="19"/>tores 
            deus non audit sed siquis 
            <lb ed="#K" n="20"/>dei cultor est et eius voluntatem 
            <lb ed="#K" n="21"/>facit hunc exaudiet</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e489">
          <lb ed="#K" n="22"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e554">Iacobus apostolus epistola i capitulo 5 fi</ref> 
          <lb ed="#K" n="23"/> <quote xml:id="bHY6yh-de4030-Qd1e493">Orate pro inuicem vt saluemi mul<lb ed="#K" break="no" n="24"/>tum 
            valet deprecatio assidua</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e461">
          <lb ed="#K" n="25"/> <ref xml:id="bHY6yh-de4030-Rd1e522">Paulus apostolus epistola i ad corinth capitulo 14</ref> 
          <lb ed="#K" n="26"/> <quote xml:id="bHY6yh-de4030-Qd1e465">Si orem lingua et spiritus meus non 
            <lb ed="#K" n="27"/>orat mens mea sine fructum 
            <lb ed="#K" n="28"/>est</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e473">
          / <ref xml:id="bHY6yh-de4030-Rd1e534">Idem in epistola ad coll capitulo 3</ref> <quote xml:id="bHY6yh-de4030-Qd1e475">et pe Quem
            <lb ed="#K" n="29"/>quodcumque facitis verbo aut opere 
            <lb ed="#K" n="30"/>omnia in nomine domini iesu christi facite</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e512">
          <lb ed="#K" n="31"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e581">Cato capitulo i</ref> <quote xml:id="bHY6yh-de4030-Qd1e516">Supplica deo</quote>
        </p> 
        <p xml:id="bHY6yh-de4030-d1e499">
          <lb ed="#K" n="32"/> ¶ <ref xml:id="bHY6yh-de4030-Rd1e566">Boetius de consolatione libro 5 et fi 
            <lb ed="#K" n="33"/>capitulo 11</ref> <quote xml:id="bHY6yh-de4030-Qd1e503">In deo <!--In deo is crossed out-->praeces ponite 
              <lb ed="#K" n="34"/>cum recte sunt inefficaces esse 
              <lb ed="#K" n="35"/>non possunt</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e520">
          <lb ed="#K" n="36"/> /<ref xml:id="bHY6yh-de4030-Rd1e591">Augustinus in epig prospi 89 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e524">fidelir sup<lb ed="#K" break="no" n="37"/>plicans 
            deo pro necessitatibus 
            <lb ed="#K" n="38"/>instius vite et misericorditur 
            <lb ed="#K" n="39"/>auditur et misicorditur non au<lb ed="#K" break="no" n="40"/>ditur 
            quid enim vtile sit infirmo 
            <cb ed="#K" n="b"/>
            <lb ed="#K" n="41"/>infirmo sit magis nouit medicus
            <lb ed="#K" n="42"/>quam egrottus Si ant postu<lb ed="#K" break="no" n="43"/>lat 
            id quod deus praecipit fie omnino 
            <lb ed="#K" n="44"/>quod poscit</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e538">
          /<ref xml:id="bHY6yh-de4030-Rd1e609">Idem 91 capitulo</ref> <quote xml:id="bHY6yh-de4030-Qd1e540">Deus cum al<lb ed="#K" break="no" n="45"/>iquid 
            male positum dando irascitur 
            <lb ed="#K" n="46"/>et non dando miseretur</quote>
        </p>
        <p xml:id="bHY6yh-de4030-d1e546">
          <lb ed="#K" n="47"/> <ref xml:id="bHY6yh-de4030-Rd1e621">Isidorus soliloquiorum libro 2 
            <lb ed="#K" n="48"/>capitulo 6</ref> <quote xml:id="bHY6yh-de4030-Qd1e550">Immundus spiritus orationis 
              <lb ed="#K" n="49"/>instantia vincitur demonia 
              <lb ed="#K" n="50"/>oratione <del rend="strikethrough">vincitur oratione demonia</del> 
              <lb ed="#K" n="51"/>superantur</quote>
        </p>
      </div>
    </body>
