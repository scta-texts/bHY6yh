         <div xml:id="bHY6yh-d52908"><!-- p3l2-r12 -->
            <head xml:id="bHY6yh-d52908-Hd1e108">Rubrica 12</head>
            <head xml:id="bHY6yh-d52908-Hd1e111" type="question-title">De partium morali consideratione</head>
            <p xml:id="bHY6yh-d52908-d1e114">
              De par<lb ed="#B" break="no"/>tium 
              morali consideratione Rubrica 12 
            </p>
            <p xml:id="bHY6yh-d52908-d1e121">
              <pb ed="#B" n="89-v"/>
              <cb ed="#B" n="b"/>
              <lb ed="#B"/><ref xml:id="bHY6yh-d52908-Rd1e118">PAulus apostolus epistola ad galatas 
              <lb ed="#B"/>capitulo 5</ref> ¶ <quote xml:id="bHY6yh-d52908-Qd1e127">Modicum fermenti totam 
              <lb ed="#B"/>massam corumpit</quote>
            </p>
            <p xml:id="bHY6yh-d52908-d1e133">
              <lb ed="#B"/><ref xml:id="bHY6yh-d52908-Rd1e133">GAufridus in<!--line jumps up--> poetria capitulo 3</ref> <quote xml:id="bHY6yh-d52908-Qd1e137">pars<!--line jumps down--> si qua sedebit 
              <lb ed="#B"/>inepte ¶ Tota trahet series ex 
              <lb ed="#B"/>illa parte pudorem ¶ Fel modi<lb ed="#B" break="no"/>cum 
              totum mel amarizat vnica 
              <lb ed="#B"/>menda ¶ totalem faciem deformat</quote>
            </p>
            <p xml:id="bHY6yh-d52908-d1e150">
              <lb ed="#B"/><ref xml:id="bHY6yh-d52908-Rd1e152">IAcobus beneuentanus capitulo 50</ref> <quote xml:id="bHY6yh-d52908-Qd1e154">Ad 
              <lb ed="#B"/>se sinceram pars uitiosa trahit</quote>
            </p>
         </div>
